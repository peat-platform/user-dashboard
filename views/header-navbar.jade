      ul#menu-version1.nav-menu
        li#menu-item-26.menu-item.menu-item-type-post_type.menu-item-object-page.current-menu-item.page_item.page-item-4.current_page_item.menu-item-26
          a(href='/user/') #{user}'s Home
        li#menu-item-211.menu-item.menu-item-type-post_type.menu-item-object-page.menu-item-211
          a(href='/user/apps') My Apps
        li#menu-item-21.menu-item.menu-item-type-post_type.menu-item-object-page.menu-item-21
          a(href='/user/registerClient') Register Client
        li#menu-item-22.menu-item.menu-item-type-post_type.menu-item-object-page.menu-item-21
          a(href='/user/') ...
        li#menu-item-25.menu-item.menu-item-type-post_type.menu-item-object-page.menu-item-23
          a(id="btn-logout-nav", href='/#') Logout
  // #site-navigation
  .header-search
    form#searchform(role='search', method='get', action='/user/search')
      div
        label.screen-reader-text(for='s') Search for:
        input#s(type='text', value='', name='s')
        input#searchsubmit(type='submit', value='Search')