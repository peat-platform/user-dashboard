    a(target='blank', href='http://www.youtube.com/openiict', title='OPENi Youtube Account')
      img(src='/wp-content/themes/openi/images/youtube.png', alt='Youtube')
    a(href='http://www.delicious.com/openi_ict', target='_blank', title='OPENi Delicious')
